{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/写作/(.*)",
      "dest": "/写作/$1"
    },
    {
      "src": "/lib/(.*)",
      "dest": "/lib/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 